    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build and install",
            "program": "dotnet",
            "args": ["${workspaceFolder}/../ffxivapp/FFXIVAPP.Client/bin/Debug/netcoreapp3.0/FFXIVAPP.Client.dll"],
            "cwd": "${workspaceFolder}/../ffxivapp/FFXIVAPP.Client",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "name": "FFXIVAPP",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build and install",
            "program": "${workspaceFolder}/../ffxivapp/FFXIVAPP.Client/bin/Debug/netcoreapp3.0/FFXIVAPP.Client.dll",
            "args": [],
            "cwd": "${workspaceFolder}/../ffxivapp/FFXIVAPP.Client",
            "console": "internalConsole",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}